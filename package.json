{
  "name": "ngx-shipper-portal-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "dev": "npm start",
    "dev:https": " HTTPS=true SSL_CRT_FILE=.cert/server.crt SSL_KEY_FILE=.cert/server.key npm start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --ci --coverage --watchAll=false --reporters=default --reporters=jest-junit",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "gen:cert": "scripts/cert.sh"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.5",
    "@heroicons/react": "^2.0.13",
    "@hookform/resolvers": "^2.9.10",
    "@popperjs/core": "^2.11.6",
    "@tanstack/query-sync-storage-persister": "^4.36.1",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@tanstack/react-query-persist-client": "^4.36.1",
    "axios": "^1.2.2",
    "classnames": "^2.3.2",
    "color": "^4.2.3",
    "html2pdf.js": "^0.10.1",
    "luxon": "^3.4.2",
    "oidc-client-ts": "^2.2.0",
    "react": "^18.2.0",
    "react-barcode": "^1.4.6",
    "react-colorful": "^5.6.1",
    "react-datepicker": "^4.10.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.41.5",
    "react-hot-toast": "^2.4.0",
    "react-oidc-context": "^2.2.0",
    "react-popper": "^2.3.0",
    "react-router-dom": "^6.4.4",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@mswjs/data": "^0.11.2",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/node-logger": "^6.5.15",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/color": "^3.0.3",
    "@types/jest": "^27.5.2",
    "@types/luxon": "^3.3.2",
    "@types/node": "^16.18.4",
    "@types/react": "^18.0.25",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "autoprefixer": "^10.4.13",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-storybook": "^0.6.8",
    "jest-junit": "^15.0.0",
    "msw": "^0.49.2",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "displayName": "Shipper Portal",
    "collectCoverageFrom": [
      "./src/**"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.{ts,tsx}"
    ]
  },
  "jest-junit": {
    "outputDirectory": "coverage",
    "outputName": "shipper-portal-test-coverage.xml",
    "coverageReporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "reports/results",
          "outputName": "shipper-portal-test-results.xml"
        }
      ]
    ]
  },
  "engines": {
    "npm": "~7"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
